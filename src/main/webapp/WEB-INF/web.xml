<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
  
  <servlet>
    <servlet-name>MyService</servlet-name>
    <servlet-class>edu.arizona.biosemantics.micropie.web.server.rpc.MyService</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>MyService</servlet-name>
    <url-pattern>/micropieweb/myService</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MicroPIEWebService</servlet-name>
    <servlet-class>edu.arizona.biosemantics.micropie.web.server.rpc.MicroPIEWebService</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>MicroPIEWebService</servlet-name>
    <url-pattern>/micropieweb/MicroPIEWebService</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
